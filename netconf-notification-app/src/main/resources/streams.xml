<streams>
    <stream>
        <name>NETCONF</name>
        <description>default NETCONF event stream</description>
        <replaySupport>true</replaySupport>
        
    </stream>
    <stream>
        <name>ALARM</name>
        <description>ALARM event stream</description>
        <replaySupport>true</replaySupport>
    </stream>
    <stream>
        <name>CONFIG_CHANGE</name>
        <description>CONFIGURATION CHANGES event stream</description>
        <replaySupport>true</replaySupport>
    </stream>
    <stream>
       <name>STATE_CHANGE</name>
       <description>STATE CHANGES event stream</description>
       <replaySupport>true</replaySupport>
    </stream>
    <stream>
       <name>SYSTEM</name>
       <description>System level notifications of the NETCONF server</description>
       <replaySupport>true</replaySupport>
    </stream>
    <stream>
       <name>HA</name>
       <description>High Availability event stream</description>
       <replaySupport>true</replaySupport>
    </stream>
</streams>