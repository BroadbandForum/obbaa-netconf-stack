<jukebox xmlns="http://example.com/ns/example-jukebox" xmlns:xc="urn:ietf:params:xml:ns:netconf:base:1.0" xmlns:ncext="http://www.test-company.com/solutions/nc-stack-extensions">
	<library>
		<artist>
			<name>Lenny</name>
			<album ncext:visibility="false" xc:operation="create">
				<name>Invisible album</name>
				<song>
					<format>mp3</format>
					<location>desktop/mymusic</location>
					<name>Are you gonne go my way</name>
				</song>
				<year>2000</year>
			</album>
			<album ncext:visibility="true" xc:operation="create">
				<name>Visible album</name>
				<song>
					<format>mp3</format>
					<location>desktop/mymusic</location>
					<name>Are you gonne go my way</name>
				</song>
				<year>2000</year>
			</album>
		</artist>
	</library>
</jukebox>