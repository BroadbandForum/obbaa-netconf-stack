<jukebox xmlns="http://example.com/ns/example-jukebox" xmlns:xc="urn:ietf:params:xml:ns:netconf:base:1.0">
	<library>
		<artist>
		    <name>Lenny</name>
			<album xc:operation="create">
				<name>A new song</name><!-- Matcher -->
				<song>
					<name>Hi there</name>
				</song>
				<song>
					<name>Hi there, yet another song</name>
				</song>
			</album>
			<album xc:operation="delete">
				<name>Circus</name><!-- Matcher -->
			</album>
		</artist>
	</library>
</jukebox>